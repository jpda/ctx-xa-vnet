{
	"$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"virtualNetworkName": {
			"type": "string"
		},
		"virtualNetworkResourceGroup": {
			"type": "string"
		},
		"newVnetRegion": {
			"type": "string"
		},
		"newVnetAddressRange": {
			"type": "string",
			"defaultValue": "10.0.0.0/20"
		},
		"newSubnetName": {
			"type": "string",
			"defaultValue": "subnet1"
		},
		"newSubnetAddressRange": {
			"type": "string",
			"defaultValue": "10.0.0.0/24"
		}
	},
	"variables": {},
	"resources": [],
	"outputs": {
		"virtualNetworkInfo": {
			"value": "[reference(concat('Microsoft.Network/virtualNetworks/', parameters('virtualNetworkResourceGroup'), '/', parameters('virtualNetworkName')), providers('Microsoft.Network', 'virtualNetworks').apiVersions[0])]",
			"type": "object"
		}
	}
}